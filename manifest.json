{
  "manifest_version": 2,

  "name": "FB Ignore Likes",
  "description": "This extension removes notifications and counts for likes",
  "version": "1.0",

 "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["background.js"]
  }],

  "page_action": {
    "default_icon": "icon.png",
    "default_title": "Click here!"
  },
  "permissions": [
    "tabs"
  ]
}
